{
    "version": 2,
    "buildCommand": "pnpm run build",
    "crons": [
      {
        "path": "/api/taskReminder/yes",
        "schedule": "*/10 * * * *"
      }
    ],
    "builds": [
      {
        "src": "api/taskReminder/yes.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  